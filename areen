#include<stdio.h>
#include<cs50.h>
int main (void){

int hight= get_hight("What is the hight?\n");
printf("the hight %i is\n",hight);
for(int i=0;i<hight;i++){
            for(int a=hight;a>i;a--)
                System.out.print(" ");
            for(int j=0;j<=i;j++)
                System.out.print("#");
            System.out.println("");
            }
return 0;
}
